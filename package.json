{
  "name": "dot-inspection",
  "version": "1.0.0",
  "description": "Sample view of dot inspections",
  "author": "",
  "license": "",
  "scripts": {
    "start": "ts-node bootstrap.ts",
    "test": "nyc --clean --all --require ts-node/register --require reflect-metadata/Reflect --extension .ts -- mocha --exit --timeout 5000",
    "test:all": "npm test **/*.spec.ts",
    "update": "updates --update --minor && npm install",
    "postupdate": "git diff-files --quiet package-lock.json || npm test"
  },
  "dependencies": {
    "body-parser": "1.20.2",
    "express": "4.18.2",
    "helmet": "^7.0.0",
    "inversify": "6.0.1",
    "inversify-binding-decorators": "4.0.0",
    "inversify-express-utils": "^6.4.3",
    "inversify-logger-middleware": "3.1.0",
    "mongodb": "6.0.0",
    "morgan": "1.10.0",
    "pg": "^8.11.3",
    "reflect-metadata": "0.1.13",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.38",
    "@types/body-parser": "^1.19.2",
    "@types/chai": "4.3.5",
    "@types/express": "^4.17.17",
    "@types/helmet": "^4.0.0",
    "@types/mocha": "10.0.1",
    "@types/mongodb": "4.0.6",
    "@types/morgan": "1.9.5",
    "chai": "4.3.8",
    "mocha": "^10.2.0",
    "nyc": "15.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "updates": "^14.5.0"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.ts"
    ]
  }
}
